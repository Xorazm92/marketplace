[[workflows.workflow]]
name = "Backend Dev Server"
author = 45055370
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend-main"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx prisma generate"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run start:dev"

