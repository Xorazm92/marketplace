modules = ["nodejs-20", "bash"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Clean Frontend Start"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Frontend"

[[workflows.workflow]]
name = "Frontend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd front-main && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Start Backend"
author = 47594518
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend-main && npm install && npm run start:dev"

[[workflows.workflow]]
name = "Frontend Fixed"
author = 47594518
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd front-main && npm install && npm run dev"

[[workflows.workflow]]
name = "Frontend Clean Start"
author = 47594518
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd front-main && rm -rf .next && npm run dev"

[[workflows.workflow]]
name = "Clean Frontend Start"
author = 47594518
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd front-main && rm -rf .next && npm install && npm run dev"

[[ports]]
localPort = 5000
externalPort = 80
