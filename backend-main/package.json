  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "node dist/simple-main",
    "start:dev": "nest start --watch --exec \"node dist/simple-main\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/simple-main",
    "start:simple": "nest build && node dist/simple-main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:cov": "jest --config jest.config.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.config.json --runInBand",
    "test:e2e": "jest --config jest.config.json --testPathPattern=e2e",
    "test:unit": "jest --config jest.config.json --testPathPattern=spec",
    "test:security": "jest --config jest.config.json --testPathPattern=security",
    "test:performance": "jest --config jest.config.json --testPathPattern=performance",
    "test:integration": "jest --config jest.config.json --testPathPattern=integration",
    "test:monitoring": "jest --config jest.config.json --testPathPattern=monitoring",
    "test:comprehensive": "jest --config jest.config.json --runInBand --verbose",
    "seed": "ts-node prisma/seed.ts",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "images:optimize": "node scripts/optimize-images.js optimize",
    "images:manifest": "node scripts/optimize-images.js manifest",
    "images:product": "node scripts/optimize-images.js product",
    "data:import": "node scripts/import-products.js import",
    "data:clear": "node scripts/import-products.js clear",
    "data:reset": "node scripts/import-products.js reset"
  },
