# Test Environment Variables
NODE_ENV=test
PORT=4001

# Test Database (SQLite for fast testing)
DATABASE_URL="file:./test.db"

# JWT Configuration
JWT_ACCESS_SECRET=test_jwt_access_secret_key_for_inbola_marketplace
JWT_REFRESH_SECRET=test_jwt_refresh_secret_key_for_inbola_marketplace
JWT_ACCESS_EXPIRES=15m
JWT_REFRESH_EXPIRES=7d

# Test API Keys (Mock values)
CLICK_SERVICE_ID=test_click_service
CLICK_SECRET_KEY=test_click_secret
CLICK_API_URL=https://test-click-api.test

PAYME_MERCHANT_ID=test_payme_merchant
PAYME_SECRET_KEY=test_payme_secret
PAYME_API_URL=https://test-payme-api.test

# SMS Configuration (Test mode)
TWILIO_ACCOUNT_SID=test_twilio_sid
TWILIO_AUTH_TOKEN=test_twilio_token
TWILIO_PHONE_NUMBER=+998900000000

# Email Configuration (Test mode)
SMTP_HOST=smtp.test.com
SMTP_PORT=587
SMTP_USER=test@example.com
SMTP_PASS=test_password

# Google OAuth (Test credentials)
GOOGLE_CLIENT_ID=test_google_client_id
GOOGLE_CLIENT_SECRET=test_google_client_secret

# Telegram Bot (Test credentials)
TELEGRAM_BOT_TOKEN=test_telegram_bot_token
TELEGRAM_BOT_USERNAME=test_bot

# Redis (Mock for testing)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=test_redis_password

# File Upload Configuration
UPLOAD_PATH=./test/uploads
MAX_FILE_SIZE=5242880

# Security Configuration
BCRYPT_ROUNDS=4
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# Child Safety Configuration
CONTENT_FILTER_ENABLED=true
PARENTAL_CONTROL_ENABLED=true
AGE_VERIFICATION_REQUIRED=true

# Test Feature Flags
ENABLE_DEBUG_LOGS=true
ENABLE_TEST_ENDPOINTS=true
MOCK_EXTERNAL_SERVICES=true